class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int> ans;
        unordered_map<int,int> mp;
        for(int i=0;i<nums.size();i++){
            if(mp.count(nums[i])>0)mp[nums[i]]++;
            else{mp[nums[i]]=1;}
        }
        for(int i=0;i<nums.size();i++){
            if(mp.count(nums[i])>0 && mp[nums[i]]!=1){
                ans.push_back(nums[i]);
                mp.erase(nums[i]);
            }
        }
        return ans;
    }
};
