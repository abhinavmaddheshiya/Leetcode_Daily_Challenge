class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int size = nums.size();
        for(int j=0;j<size;j++)
        {int temp=nums[j];
        for(int i=0;i<size;i++){
            if(nums[i]+temp==target && i!=j){
                vector<int> ans;
                ans.push_back(j);
                ans.push_back(i);
                return ans;
            }
        }}
        vector<int> v;
        return v;
    }
};
